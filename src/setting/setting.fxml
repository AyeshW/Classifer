<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="300.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="setting.settingController">
   <children>
      <Button layoutX="92.0" layoutY="263.0" mnemonicParsing="false" onAction="#change" prefHeight="25.0" prefWidth="117.0" text="Change Password" />
      <PasswordField fx:id="currPass" layoutX="64.0" layoutY="121.0" prefHeight="25.0" prefWidth="173.0" promptText="Current Password" />
      <PasswordField fx:id="newPass" layoutX="64.0" layoutY="164.0" prefHeight="25.0" prefWidth="173.0" promptText="New Password" />
      <PasswordField fx:id="confNewPass" layoutX="64.0" layoutY="207.0" prefHeight="25.0" prefWidth="173.0" promptText="Confirm Password" />
      <TextField fx:id="username" layoutX="64.0" layoutY="80.0" prefHeight="25.0" prefWidth="173.0" promptText="User Name" />
      <HBox prefHeight="57.0" prefWidth="350.0" style="-fx-background-color: #9eafaf;">
         <children>
            <Label prefHeight="400.0" prefWidth="300.0" text="Settings" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold Italic" size="27.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
